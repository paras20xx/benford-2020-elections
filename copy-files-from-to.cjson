            "to": "dump/alabama/vote-count/alabama.xlsx"
        },
        {
            // https://chicagoelections.gov/en/election-results-specifics.asp
            "from": "https://chicagoelections.gov/en/data-export.asp?election=251&race=11&ward=&precinct=",
            "to": "dump/chicago/vote-count/chicago.xls"
        },
        {
            // https://results.enr.clarityelections.com/CO/105975/web.264614/#/summary
            "from": "https://results.enr.clarityelections.com//CO//105975/270054/reports/detailxml.zip",
            "to": "dump/colorado/vote-count/colorado.zip"
        },
        {
            // https://results.enr.clarityelections.com/GA/105369/web.264614/#/reporting
            "from": "https://results.enr.clarityelections.com//GA//105369/270190/reports/detailxml.zip",
            "to": "dump/georgia/vote-count/georgia.zip"
        },
        {
            // https://electionresults.iowa.gov/IA/106279/web.264614/#/summary
            "from": "https://electionresults.iowa.gov//IA//106279/270661/reports/detailxml.zip",
            "to": "dump/iowa/vote-count/iowa.zip"
        },
        {
            // https://results.enr.clarityelections.com/KY/106379/web.264614/#/summary
            "from": "https://results.enr.clarityelections.com//KY//106379/270707/reports/detailxml.zip",
            "to": "dump/kentucky/vote-count/kentucky.zip"
        },
        {
            // https://mielections.us/election/results/2020GEN_CENR.html
            "from": "https://mielections.us/election/results/DATA/2020GEN_MI_CENR_BY_COUNTY.xls",
            "to": "dump/michigan/vote-count/michigan.xls"
        },
        {
            // https://county.milwaukee.gov/EN/County-Clerk/Off-Nav/Election-Results/Election-Results-Fall-2020
            "from": "https://raw.githubusercontent.com/FraudAnalysis/Benford2020/main/data/milwaukee_county.csv",
            "to": "dump/milwaukee/vote-count/milwaukee_copied.csv"

            // Note: dump/milwaukee/vote-count/milwaukee.csv was generated directly from the official site and that is used for final calculations
        },
        {
            // https://electionresults.nebraska.gov/resultsSW.aspx?text=Race&type=PRS&map=CTY
            "from": "https://electionresults.nebraska.gov/ResultsExport.aspx?rid=11232&pty=&osn=90&name=For%20President%20and%20Vice%20President%20of%20the%20United%20States&cat=CTYALL",
            "to": "dump/nebraska/vote-count/nebraska.xls"
        },
        {
            // https://results.vote.wa.gov/results/20201103/export.html
            "from": "https://results.vote.wa.gov/results/20201103/export/20201103_AllStatePrecincts.csv",
            "to": "dump/washington/vote-count/washington.csv"
        },
        {
            "from": "https://raw.githubusercontent.com/davidbau/seedrandom/released/seedrandom.js",
            "to": "utils/seedrandom/seedrandom.js"
        }
    ],
    "copyFilesSettings": {
        "whenFileExists": "do-nothing",
        "removeSourceMappingURL": false,
        "addReferenceToSourceOfOrigin": true
    }
}